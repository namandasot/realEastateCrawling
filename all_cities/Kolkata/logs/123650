arya","original_number":null,"number":"8697275549","profile_uuid":"d7b340bf-5365-468d-bb0a-000a30033dc4","client_uuid":"ed72d12b-6876-442d-aac4-d1e2a26e447d","profile_picture_url":null,"chat_enabled":false,"profile_type":"Owner"}],"contact_person_type":"Landlord","price_trends":{"trend":[{"q3":2864,"q4":2996,"year":2014},{"q1":2710,"q3":3038,"q2":2558,"year":2015}],"avg_price_per_sqft":3038,"percent_growth":1.4,"property_type_id":1,"number_of_years":1},"amenities_hash":{"has_swimming_pool":false,"has_gym":false,"has_gas_pipeline":false,"has_servant_room":false,"is_gated_community":true,"number_of_lifts":0,"parking_count":0,"power_backup_type":"None","water_supply_type":null,"has_garden":false,"has_kids_area":false,"has_sports_facility":false},"polygon_data":{"primary_polygon_uuid":"31c4cfc97fd3f1b4809b","primary_polygon_url_name":"ariadaha","primary_polygon_name":"Ariadaha","parent_polygon_uuid":"90d5af399d7256af0694","parent_polygon_url_name":"kolkata","parent_polygon_name":"Kolkata"}}}